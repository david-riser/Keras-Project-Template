{
    "exp": {
	"name": "cifar_deepcluster"
    },
    "data_loader": {
	"name": "cifar_data_loader.CifarDataLoader",
	"batch_size": 64,
	"preprocessing_function": "resnet50.preprocess_input"
    },
    "model":{
	"name": "cifar_deepcluster_model.CifarDeepClusterModel",
	"learning_rate": 0.01,
	"optimizer": "SGD",
	"momentum":0.9,
	"backbone": "resnet50.ResNet50",
	"pooling": "max",
	"n_clusters":10
    },
    "trainer":{
	"name": "cifar_deepcluster_trainer.CifarDeepClusterTrainer",
	"kmeans_epochs": 5,
	"kmeans_batches_per_epoch": 100,
	"batches":8000,
	"target_updates":35,
	"verbose_training": true
    },
    "callbacks":{
	"checkpoint_monitor": "val_loss",
	"checkpoint_mode": "min",
	"checkpoint_save_best_only": true,
	"checkpoint_save_weights_only": true,
	"checkpoint_verbose": true
    },
    "evaluators":[
	{
	    "name": "cifar_evaluator.CifarEvaluator",
	    "output_name": "/home/ubuntu/cifar_test_images.pdf",
	    "nrows": 6,
	    "ncols": 5,
	    "samples": 320,
	    "dpi": 100
	}
    ]
}
