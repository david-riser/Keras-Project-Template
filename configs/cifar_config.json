{

    "exp": {
	"name": "cifar"
    },
    
    "model":{
	"learning_rate": 0.001,
	"optimizer": "adam"
    },

    "data_loader":{
	"batch_size": 64
    },

    "trainer":{
	"num_epochs": 20,
	"verbose_training": true
    },

    "callbacks":{
	"checkpoint_monitor": "val_loss",
	"checkpoint_mode": "min",
	"checkpoint_save_best_only": true,
	"checkpoint_save_weights_only": true,
	"checkpoint_verbose": true,
	"tensorboard_write_graph": false
    }
}
